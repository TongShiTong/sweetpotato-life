/* components/navigation/index.wxss */
.navbar {
  width: 100%;
  position: fixed;
  top: 0;
  z-index: 999;
  font-size: 34rpx;
  font-weight: 500;
  text-align: center;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}

.nav_box {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 88rpx;
}
.back {
  height: 44rpx;
  width: 44rpx;
  /* background: #f0f; */
  flex-shrink: 0;
  margin-left: 32rpx;
}
.back image {
  width: 44rpx;
  height: 44rpx;
}
.nav_title {
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.nav_title_img image {
  width: 136rpx;
  height: 48rpx;
  vertical-align: middle;
}
